      "title": "API Latency (ms)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(k6_http_req_duration_ms_sum[1m]) / rate(k6_http_req_duration_ms_count[1m])",
          "legendFormat": "latency_ms",
          "refId": "A"
        }
      ]
